{
  "name": "book-search",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines":{"node": "12.x"},
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "postinstall": "npm run seed",
    "heroku-postbuild": "npm run build",
    "seed": "cd seeds && node seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtbanh/search-book.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mongodb": "^3.6.0",
    "mongoose": "^5.10.0",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.3.4"
  },
  "bugs": {
    "url": "https://github.com/jdavis3333/book-search/issues"
  },
  "homepage": "https://github.com/jdavis3333/book-search#readme"
}
